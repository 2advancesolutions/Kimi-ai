# API Configuration
# Copy this file to .env and update with your actual API Gateway URL

# API Gateway URL (replace with your actual URL)
REACT_APP_API_URL=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/prod

# API Type (api-gateway or lambda-url)
REACT_APP_API_TYPE=api-gateway

# Development settings
REACT_APP_DEV_MODE=true

# Optional: Custom API Gateway URL for different environments
# REACT_APP_API_GATEWAY_URL=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/prod

# Optional: Lambda Function URL (fallback)
# REACT_APP_LAMBDA_URL=https://your-lambda-url.lambda-url.region.on.aws

# Optional: Enable detailed logging
# REACT_APP_DEBUG=true